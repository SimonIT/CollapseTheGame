<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit trunk//EN"
	"http://www.gwtproject.org/doctype/2.8.2/gwt-module.dtd">
<module rename-to="html">
	<inherits name='ch.asynk.gdx.boardgame'/>
	<inherits name='com.badlogic.gdx.ai'/>

	<inherits name='CollapseTheGame'/>
	<entry-point class='de.nilsim.collapse.client.HtmlLauncher'/>
	<set-configuration-property name='xsiframe.failIfScriptTag' value='FALSE'/>
	<set-configuration-property name="gdx.assetpath" value="../android/assets"/>
	<set-property name="user.agent" value="gecko1_8, safari"/>
	<collapse-property name="user.agent" values="*"/>
</module>
